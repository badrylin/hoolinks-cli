{
  "name": "hoolinks-cli",
  "version": "2.0.2",
  "description": "昊链专用脚手架工具",
  "scripts": {
    "compile": "tsc --build ./scripts/tsconfig.json -w",
    "compile:build": "tsc --build ./scripts/tsconfig.json",
    "dev": "node ./bin/index.js dev --env=test --apps=app1 --uniqueName=name",
    "build": "node ./bin/index.js build --env=test --apps=app1,app2 --uniqueName=name",
    "build:cdn": "node ./bin/index.js build --env=test --apps=app1,app2 --uniqueName=name --cdn=http://127.0.0.1:8081/dist/",
    "dll": "node ./bin/index.js dll",
    "release": "node ./release.cjs",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path ."
  },
  "author": "linzeqin <810390198@qq.com>",
  "license": "ISC",
  "typings": "./scripts_/utils/config.d.ts",
  "bin": {
    "hoolinks": "./bin/index.js"
  },
  "browserslist": [
    "ie 10",
    "last 2 version"
  ],
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
