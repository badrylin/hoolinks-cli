{
  "name": "hoolinks-cli",
  "version": "1.1.2",
  "description": "昊链专用脚手架工具",
  "scripts": {
    "compile": "tsc --build ./scripts/tsconfig.json -w",
    "dev": "node ./bin/index.js dev -p --env=test --apps=app1,app2 --uniqueName=name",
    "build": "node ./bin/index.js build --env=test --apps=app1,app2 --uniqueName=name",
    "build:cdn": "node ./bin/index.js build --env=test --apps=app1,app2 --uniqueName=name --cdn=http://127.0.0.1:8080/",
    "dll": "node ./bin/index.js dll"
  },
  "author": "linzeqin <810390198@qq.com>",
  "license": "ISC",
  "bin": {
    "hoolinks": "./bin/index.js"
  },
  "files": [
    "bin",
    "scripts_"
  ],
  "browserslist": [
    "ie 10",
    "last 2 version"
  ],
